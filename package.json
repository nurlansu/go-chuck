{
  "name": "go-chuck",
  "version": "1.0.1",
  "description": "",
  "license": "MIT",
  "author": "Nurlan Su",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nurlansu/go-chuck.git"
  },
  "scripts": {
    "autoprefixer": "postcss -c package.json -r ./public/static/css/*.css -u autoprefixer",
    "sass": "wt compile ./public/source/sass/*.scss -b ./public/static/css/ -s compressed",
    "sass:ci": "node-sass ./public/source/sass/*.scss -o ./public/static/css/ --output-style compressed",
    "sass:watch": "wt watch ./public/source/sass/*.scss -b ./public/static/css/ -s compressed",
    "build": "npm run sass && npm run autoprefixer",
    "build:ci": "npm run sass:ci && npm run autoprefixer"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^9.5.0",
    "node-sass": "^4.11.0",
    "postcss-cli": "^6.1.2"
  },
  "autoprefixer": {
    "browsers": "> 1%, last 2 versions, ie >= 9"
  }
}
